# Builder API Configuration (for Agent Zero system)
# This API is used during agent construction

BUILDER_PROVIDER=openai
BUILDER_MODEL=gpt-4o
BUILDER_API_KEY=your-builder-api-key-here
BUILDER_BASE_URL=https://api.openai.com/v1
BUILDER_TIMEOUT=60
BUILDER_MAX_RETRIES=3
BUILDER_TEMPERATURE=0.7

# Runtime API Configuration (for generated agents)
# This configuration will be copied to generated agent projects

RUNTIME_PROVIDER=openai
RUNTIME_MODEL=gpt-3.5-turbo
RUNTIME_API_KEY=your-runtime-api-key-here
RUNTIME_BASE_URL=https://api.openai.com/v1
RUNTIME_TIMEOUT=30
RUNTIME_TEMPERATURE=0.7
